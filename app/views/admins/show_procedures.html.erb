All Procedures | <%= link_to "Pending Procedures", admins_path %>

<div>
  <% @procedures.each do |procedure|%>
    <div>
      <%=procedure.name%> <%= procedure.date %> <%= procedure.description %> <%= procedure.price %> 
        <%= procedure.status %> <%= procedure.facility_id %>
      <%= link_to "Edit", edit_procedure_path(procedure.id) %>
      <%= link_to "Delete", procedure_path(procedure.id), method: :'delete', data: { confirm: 'Are you sure?'} %>
    </div>
  <%end%>
</div>